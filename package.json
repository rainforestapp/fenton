{
  "name": "fentonjs",
  "version": "0.1.0",
  "description": "A http library that uses fetch to fetch your server side deer",
  "repository": "https://github.com/rainforestapp/fentonjs",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm test",
    "prepublish": "npm run build",
    "build": "babel ./index.js ./dist"
  },
  "keywords": [
    "fetch",
    "http"
  ],
  "author": "RainforestQA Eng",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^12.1.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint-config-rainforest": "^0.1.6"
  },
  "dependencies": {
    "es6-deferred": "^1.3.0",
    "event-emitter": "^0.3.4",
    "lodash": "3.10.1",
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      ".*mock.*",
      ".*stub.*"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      ".*mock.*",
      ".*stub.*"
    ]
  }
}
